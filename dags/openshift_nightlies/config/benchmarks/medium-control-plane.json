{
    "benchmarks": [
        {
            "name": "scale-6",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "6",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "worker"
            }
        },
        {
            "name": "node-density-heavy-1000itsPerNs",
            "workload": "kube-burner-ocp-wrapper",
            "command": "./run.sh",
            "env": {
                "ES_SERVER": "",
                "QPS": "80",
                "BURST": "80",
                "WORKLOAD": "node-density-heavy",
                "EXTRA_FLAGS": "--timeout=2h --pods-per-node=200"
            }
        },
        {
            "name": "node-density-heavy-100itsPerNs",
            "workload": "kube-burner-ocp-wrapper",
            "command": "./run.sh",
            "env": {
                "ES_SERVER": "",
                "QPS": "80",
                "BURST": "80",
                "WORKLOAD": "node-density-heavy",
                "EXTRA_FLAGS": "--timeout=2h --pods-per-node=200 --iterations-per-namespace=100"
            }
        }
    ]
}
